.container {
  width: 750rpx;
  height: auto;
  display: flex;
  flex-direction: column;
}

.logobox {
  width: 750rpx;
  height: 332rpx;
  background-color: #f8f8f8;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo {
  width: 174rpx;
  height: 174rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.logobox text {
  font-size: 28rpx;
  color: #333;
}

.info-content{
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.info-content view{
  width: 750rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #d2d2d2;
}

.info-content view text{
  font-size: 28rpx;
  color: #333;
  margin-left: 20rpx;
}
